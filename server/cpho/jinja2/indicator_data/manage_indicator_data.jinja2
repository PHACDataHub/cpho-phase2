{% extends 'base.jinja2' %}
{% block content %}
  <h1>{{ tdt("indcator data for")}} {{ indicator.name }}</h1>
  <h2>{{ dimension_type.name }}</h2>
  <form action="." method="POST">
    {{ formset.management_form }}
    {{ csrf_input }}
    {% for form in formset %}
      <div class="my-4">
        <div>{{ form.instance.dimension_value.name }}</div>
        {{ form.as_p() }}
      </div>
    {% endfor %}
    <input type="submit" value="{{ tdt('submit') }}">
  </form>
{% endblock %}
